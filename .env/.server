#Ixia Environment
IXIA_HOME=/tools/AGRtools/ixia
TCL_HOME=/tools/AGRtools/ixia
TCLver=8.4
IxiaLibPath=$IXIA_HOME/lib
IxiaBinPath=$IXIA_HOME/bin
TCLLibPath=$TCL_HOME/lib
TCLBinPath=$TCL_HOME/bin
TCL_LIBRARY=$TCLLibPath/tcl$TCLver
TK_LIBRARY=$TCLLibPath/tk$TCLver

PATH=$IxiaBinPath:$PATH
PATH=$TCLBinPath:$PATH

#TCLLIBPATH=$IxiaLibPath:$TCLLIBPath; # does not work, not a set of paths, must point to Ixia only
TCLLIBPATH=$IxiaLibPath

IXIA_RESULTS_DIR=/tmp/Ixia/Results
IXIA_LOGS_DIR=/tmp/Ixia/Logs
IXIA_TCL_DIR=$IxiaLibPath
IXIA_SAMPLES=$IxiaLibPath/ixTcl1.0
IXIA_VERSION=6.00.700.3

#Export the Ixia env variables.
export IXIA_HOME TCL_LIBRARY TK_LIBRARY TCLLIBPATH
export LD_LIBRARY_PATH IXIA_RESULTS_DIR
export IXIA_LOGS_DIR IXIA_TCL_DIR IXIA_SAMPLES
export IXIA_TCL_DIR IXIA_VERSION

#Export Path for GCC
my_gcc=/tools/AGRtools/x64/bin
PATH=$my_gcc:$PATH

#Shard Libary
LD_LIBRARY_PATH=$IxiaLibPath:$TCLLibPath:$LD_LIBRARY_PATH
export LD_LIBRARY_PATH=/home/swbuilds/linux64/jdk_17u2/lib:/tools/AGRtools/gnutools/gcc-4.8.2/lib64:/tools/AGRtools/gnutools/gcc-4.8.2/lib:/tools/AGRtools/x86_64/lib64:/tools/AGRtools/x86_64/lib:~/local/lib/:$LD_LIBRARY_PATH

